{
  "version": 2,
  "routes": [
    { "src": "/debug", "dest": "/index.html" },
    { "src": "/login", "dest": "/index.html" },
    { "src": "/register", "dest": "/index.html" },
    { "src": "/dashboard", "dest": "/index.html" },
    { "src": "/dashboard/(.*)", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite"
}
